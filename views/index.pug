block scripts
  script.
    document.addEventListener("DOMContentLoaded", function() {
      setTimeout(function() {
        var modalEl = document.getElementById('odooPartnershipModal');
        if (modalEl && typeof bootstrap !== 'undefined') {
          // Clean up any manual class manipulation from other scripts
          if (modalEl.classList.contains('show') && modalEl.style.display === 'block' && !document.querySelector('.modal-backdrop')) {
            modalEl.classList.remove('show');
            modalEl.style.display = '';
          }

          var modalInstance = bootstrap.Modal.getOrCreateInstance(modalEl);
          modalInstance.show();
        }
      }, 2500);
    });

