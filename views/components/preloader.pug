// Preloader Component
.paginacontainer
  .progress-wrap
    svg.progress-circle.svg-content(width="100%" height="100%" viewBox="-1 -1 102 102")
      path(d="M50,1 a49,49 0 0,1 0,98 a49,49 0 0,1 0,-98")

section
  #preloader
    .ctn-preloader.ctn-preloader1#ctn-preloader
      .animation-preloader
        .spinner
          img(src="images/gocloud%20logo.webp" alt="GoCloud Logo" height="200px" width="200px" loading="lazy" style="border-radius: 50%;")
        .txt-loading
          span.letters-loading(data-text-preloader="G") G
          span.letters-loading(data-text-preloader="O") O
          span.letters-loading(data-text-preloader="C") C
          span.letters-loading(data-text-preloader="L") L
          span.letters-loading(data-text-preloader="O") O
          span.letters-loading(data-text-preloader="U") U
          span.letters-loading(data-text-preloader="D") D
      .loader-section-left.loader-section.section-left
      .loader-section-right.loader-section.section-right
