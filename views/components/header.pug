// Header Component
header(role="banner")
  .header-area.header-area1.header-area-all.d-none.d-md-block.d-lg-block#header
    .container
      .row
        .col-12
          .header-elements
            div
              a(href="index.html" aria-label="GoCloud Home")
                img(src="images/gocloud%20logo.webp" alt="GoCloud Logo" height="100px" width="100px" loading="lazy")

            nav(role="navigation" aria-label="Main Navigation")
              .main-menu-ex.main-menu-ex1
                ul(role="menubar")
                  li.dropdown-menu-parrent(role="none")
                    a(href="/" role="menuitem") Home
                  li.dropdown-menu-parrent(role="none")
                    a(href="about.html" role="menuitem") About
                  li.dropdown-menu-parrent(role="none")
                    a(href="service.html" role="menuitem") Services
                  li.dropdown-menu-parrent(role="none")
                    a(href="contact.html" role="menuitem") Contact
                  li.dropdown-menu-parrent(role="none")
                    a(href="https://erp.gocloudeg.com/web/login" target="_blank" rel="noopener noreferrer" role="menuitem")
                    button.btn.bg-primary(type="button") Login

            .header1-buttons
              .contact-btn
                .icon
                  img(src="images/header1-icon.webp" alt="" loading="lazy")
                .headding
                  p Make a Call
                  a(href="tel://+201017383815" aria-label="Call us at +201017383815") +201017383815
              .button
                a.theme-btn1(href="https://gocloudeg.com/survey/start/e6323978-a3c5-430f-a14b-88d2ce1169da" aria-label="Get a Quote") Get A Quote 
                  span
                    i.fa-solid.fa-arrow-right(aria-hidden="true")

// Mobile Header
header(role="banner")
  .mobile-header.d-block.d-md-none.d-lg-none
    .container-fluid
      .col-12
        .mobile-header-elements
          .mobile-logo
            a(href="index.html" aria-label="GoCloud Home")
              img(src="images/gocloud%20logo.webp" height="100" width="100" alt="GoCloud Logo" loading="lazy")
          .mobile-nav-icon
            button.mobile-menu-toggle(type="button" aria-label="Open navigation menu" aria-expanded="false")
              i.fa-duotone.fa-bars-staggered(aria-hidden="true")

  .mobile-sidebar.d-block.d-md-none.d-lg-none(role="dialog" aria-label="Mobile navigation menu")
    .logo-m
      a(href="index.html" aria-label="GoCloud Home")
        img(src="images/gocloud%20logo.webp" height="100" width="100" alt="GoCloud Logo" loading="lazy")
    .menu-close
      button(type="button" aria-label="Close navigation menu")
        i.fa-solid.fa-xmark(aria-hidden="true")
    .mobile-nav
      nav(role="navigation" aria-label="Mobile Navigation")
        ul(role="menubar")
          li(role="none")
            a(href="index.html" role="menuitem") Home
          li(role="none")
            a(href="about.html" role="menuitem") About Us
          li(role="none")
            a(href="service.html" role="menuitem") Services
          li(role="none")
            a(href="contact.html" role="menuitem") Contact

      .mobile-button
        a.menu-btn2(href="https://erp.gocloudeg.com/web/login" aria-label="Login to ERP system") Login
          span
            i.fa-solid.fa-arrow-right(aria-hidden="true")

      .single-footer-items
        h3 Contact Us

        .contact-box
          .icon
            img(src="images/footer1-icon1.webp" alt="Phone Icon" loading="lazy")
          .pera
            a(href="tel://0233762533" aria-label="Call us at 0233762533") 0233762533

        .contact-box
          .icon
            img(src="images/footer1-icon2.webp" alt="Mobile Phone Icon" loading="lazy")
          .pera
            a(href="tel://+201017383815" aria-label="Call us at +201017383815") +201017383815

        .contact-box
          .icon
            img(src="images/footer1-icon3.webp" alt="Email Icon" loading="lazy")
          .pera
            a(href="mailto:marketing@gocloudeg.com" aria-label="Send email to marketing@gocloudeg.com") marketing@gocloudeg.com

        .contact-box
          .icon
            img(src="images/footer1-icon4.webp" alt="Website Icon" loading="lazy")
          .pera
            a(href="https://gocloudeg.com" aria-label="Visit GoCloud website") www.gocloudeg.com

      .contact-infos
        h3 Our Location
        ul.social-icon
          li
            a(href="https://www.linkedin.com/company/gocloud-co/posts/?feedView=all")
              i.fa-brands.fa-linkedin-in
          li
            a(href="https://www.google.com/search?q=gocloud+eg&sca_esv=1b465dddb89ef670&sxsrf=AE3TifPO8dDI2un8QLeuyTZoAmiw1QCmhA%3A1750690736441&ei=sGtZaLfRGoaNkdUP4_DKyAI&ved=0ahUKEwi36dOb54eOAxWGRqQEHWO4EikQ4dUDCBA&uact=5&oq=gocloud+&gs_&sclient=gws-wiz-serp")
              i.fa-brands.fa-google
          li
            a(href="https://www.youtube.com/@GoCloudeg")
              i.fa-brands.fa-youtube
